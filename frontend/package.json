{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gist-cleaner.ndolestudio.com",
  "dependencies": {
    "axios": "^0.19.2",
    "moment": "^2.29.2",
    "momentjs": "^2.0.0",
    "pusher-js": "^5.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-toastify": "^5.5.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "NODE_ENV=production postcss src/styles/tailwind.pcss -o src/styles/tailwind.css",
    "watch:css": "postcss src/styles/tailwind.pcss -o src/styles/tailwind.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && firebase deploy --only hosting"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@mdi/font": "^4.9.95",
    "autoprefixer": "^9.8.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postscribe": "^2.0.8",
    "prettier": "1.18.2",
    "pretty-checkbox": "^3.0.3",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^1.5.1"
  }
}
